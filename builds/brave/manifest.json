{
  "name": "Trackabi tracker",
  "version": "1.0",
  "description": "",
  "manifest_version": 3,
  "permissions": [
    "activeTab",
    "tabs",
    "runtime"
  ],
  "background": {
    "service_worker": "index.js"
  },
  "action": {
    "default_icon": {
      "16": "images/icon16x16.png",
      "32": "images/icon32x32.png",
      "36": "images/icon32x32.png",
      "48": "images/icon48x48.png",
      "128": "images/icon128x128.png"
    }
  }
}
